msgid "IDS_ACHIEVEMENT_DESCRIPTION_DE_JAPAN"
msgstr "Win a battle while fighting on a ship of the Empire of Japan."

msgid "IDS_ACHIEVEMENT_DESCRIPTION_DE_RUSSIA"
msgstr "Win a battle while fighting on a ship of the Russian Empire."

msgid "IDS_ACHIEVEMENT_DESCRIPTION_DREADNOUGHT_EVENT_ATBA_CALIBER"
msgstr "Destroy an enemy ship by dealing the final damage with secondary battery guns."

msgid "IDS_ACHIEVEMENT_DESCRIPTION_DREADNOUGHT_EVENT_DOUBLE_KILL"
msgstr "Destroy 2 enemy ships within %(timeInterval)s seconds."

msgid "IDS_ACHIEVEMENT_DESCRIPTION_DREADNOUGHT_EVENT_FIRST_BLOOD"
